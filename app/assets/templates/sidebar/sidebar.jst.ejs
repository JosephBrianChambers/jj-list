
<div id="tabs">
  <ul>
    <li><a href="#tabs-1">Dashboard</a></li>
    <li><a href="#tabs-2">
          <span class="glyphicon glyphicon-star star stared"> </span>
          Users
        </a>
    </li>
  </ul>
  <div id="tabs-1">
    <div class="btn-group-vertical">
     <button type="button" class="btn btn-default btn-icon" id="search-btn">
       <span class="glyphicon glyphicon-search"> </span>
       Search
     </button>
     
     <button type="button" class="btn btn-default btn-icon" id="create-post-btn">
       <span class="glyphicon glyphicon-pencil"> </span>
       Create Post
     </button>
   </div> 
   <h3 class="ban"><span class="ban-text"></span></h3>
   <div class="btn-group-vertical">
     <button type="button" class="btn btn-default btn-icon" id="favorite-post-btn">
       <span class="glyphicon glyphicon-star star stared"> </span>
       Favorite Posts
     </button>
     <button type="button" class="btn btn-default btn-icon" id="my-posts-btn">
       <span class="glyphicon glyphicon-list"> </span>
       My Posts
     </button>
    </div>
    <h3 class="ban"><span class="ban-text"></span></h3>
  </div>
  
  <div id="tabs-2">
    
    <h4>
      <span class="list title">
        <%= JjList.currentUser.get("favoriteUsers").length %> favorite users
      </span>
    </h4>
  	<ul class="list-favorite-users">
  
    <% JjList.currentUser.get("favoriteUsers").forEach(function (user) { %>
      <li>
          <a href="#users/<%= user.id %>/posts">
            <span class="list-favorite-username fake-link">
              <%= user.escape("username") %>
            </span>
          </a>
        <br>
      </li>
    <% }) %>
      
  	</ul>
  </div>
</div>
   
    
    
    
    
    
    
    

 
